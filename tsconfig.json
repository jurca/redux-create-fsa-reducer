{
  "compileOnSave": true,
  "compilerOptions": {
    "declaration": true,
    "target": "esnext",
    "module": "commonjs",
    "locale": "en",
    "moduleResolution": "node",
    "newLine": "lf",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "pretty": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true
  },
  "include": [
    "index.ts"
  ]
}
